@startuml uc39_sequence
title UC-39: Education ROI (Sequence)
autoactivate on
actor Sponsor
participant "SCP Core" as Core
participant "Vertical: Edu" as Edu
participant "Vertical: Econ" as Econ
participant "Geo-Vertical: Family" as Family
Sponsor -> Core: FindHighPotential()
    Core -> Edu: GetTopStudents()
    return ListA
    Core -> Econ: GetLowIncome(ListA)
    return ListB
    Core -> Family: CheckStability(ListB)
    return FinalList
return Show(FinalList)
@enduml
