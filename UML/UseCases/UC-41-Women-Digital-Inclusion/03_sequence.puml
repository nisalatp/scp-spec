@startuml uc41_sequence
title UC-41: Digital Inclusion (Sequence)
autoactivate on
actor NGO
participant "SCP Core" as Core
participant "Vertical: Edu" as Edu
participant "Vertical: Econ" as Econ
NGO -> Core: FindDigitalWorkers()
    Core -> Edu: GetEducatedWomen()
    return ListA
    Core -> Econ: CheckTechAccess(ListA)
    return ListB
    Core -> Core: CheckConnectivity(ListB)
    return FinalList
return Report(FinalList)
@enduml
