@startuml
title UC-10: Clean Water Access (Sequence)
autoactivate on
actor Planner
participant "Geo-Vertical: WASH" as WASH
participant "Vertical: Health" as Health
participant "SCP Core: Geo" as Geo
Planner -> WASH: AnalyzeAccess()
    WASH -> Geo: GetHouseholds()
    return HH_List
    WASH -> Geo: GetWaterPoints()
    return WP_List
    WASH -> WASH: CalcDistance(HH, WP)
    return UnservedHH
    WASH -> Health: GetDiseaseIncidence(UnservedHH)
    return HighRisk
return Map(HighRisk)
@enduml
